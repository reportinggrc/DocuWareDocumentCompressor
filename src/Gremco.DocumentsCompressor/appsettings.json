{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Error",
			"Override": {
				"Microsoft": "Warning",
				"Microsoft.Hosting.Lifetime": "Warning"
			}
		},
		"WriteTo": [
			{
				"Name": "Console"
			},
			{
				"Name": "Debug"
			},
			{
				"Name": "File",
				"Args": {
					"path": "./logs/DocumentsCompressor-.txt",
					"rollingInterval": "Day"
				}
			}
		]
	},
	"DocuWareConfiguration": {
		"ServerUrl": "",
		"Platform": "DocuWare/Platform",
		"Username": "",
		"Password": ""
	}
}
